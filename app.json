{
  "expo": {
    "name": "ASH",
    "slug": "ash",
    "version": "1.0.0",
    "description": "ASH - Architectural Site History. A tool for making photo keys a breeze.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ash",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.joey.ash",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "ASH reads your photos' location and direction metadata to accuratelyplace them on the map."
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ef630813-7e1e-418a-a0b2-e2ad32afcd8b"
      }
    }
  }
}
